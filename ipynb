   "source": [
    "#### 8. How many transactions happen in the christmas period(20th - 31st December) each year?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 95,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "date\n",
       "2013     6204\n",
       "2014    11088\n",
       "2015    10494\n",
       "2016     8745\n",
       "Name: transactions, dtype: int64"
      ]
     },
     "execution_count": 95,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Finding the number of transactions per christmas period\n",
    "christmas_transactions = train[train['date'].dt.day.between(20, 31) & train['date'].dt.month.eq(12)].groupby(train['date'].dt.year)['transactions'].count()\n",
    "christmas_transactions"
   ]
  },
  {
   "cell_type": "code",
